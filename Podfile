  use_frameworks!
  pod 'Alamofire', '~> 4.0'
  pod 'SwiftyJSON','4.0.0'
  pod 'SwiftyBeaver' ,'1.5.0'
  pod 'HandyJSON', '4.0.0-beta.1'
  
  # Pods for tinypng

  target 'tinypngTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'tinypngUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
